{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "jsx-boolean-value": false,
    "member-access": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "import-name": false,
    "interface-name": [true, "never-prefix"],
    "variable-name": false,
    "ter-arrow-parens": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["**/*.js"]
  }
}
